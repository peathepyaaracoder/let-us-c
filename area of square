// a program to print the area of a square 

#include<stdio.h>

float area(float side);

int main(){
    float side;
    printf("enter side: ");
    scanf("%f", &side);

    area(side);
    
    return 0;
}

float area(float side){
    float pavini;
    pavini = side*side;
    printf("area of square of side %f is %f", side, pavini);
}

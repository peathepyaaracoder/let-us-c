// a program to print all numbers from 0 to 100 except n (given by user)

#include<stdio.h>

int main (){
    int n,i;
    printf("enter a number: ");
    scanf("%d", &n);

    for(i=0; i<=100; i++){
        if(i== n){
            continue;
        }
        printf("%d\n", i);
    }

    return 0;
}

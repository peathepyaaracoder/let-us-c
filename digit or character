// a program to check if given character is digit or character

#include <stdio.h>
int main (){
    char ch;
    printf("enter a character: ");
    scanf("%c", &ch);

    if(ch>='0' && ch<='9'){
        printf("given character is a digit");
    }
    else{
        printf("given character is a character lol");
    }
    return 0;
}
